{
    "manifest_version": 2,
    "name": "Coursera Subtitle Translation",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": [
                "*://www.coursera.org/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "browser_action": {},
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    }
}
